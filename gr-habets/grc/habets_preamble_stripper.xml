<?xml version="1.0"?>
<block>
  <name>Preamble stripper</name>
  <key>habets_preamble_stripper</key>
  <category>[habets]</category>
  <import>import habets</import>
  <make>habets.preamble_stripper($prefix)</make>

  <param>
    <name>Prefix</name>
    <key>prefix</key>
    <type>int_vector</type>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
